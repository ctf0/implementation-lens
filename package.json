{
    "name": "implementation-lens",
    "displayName": "Implementation Lens",
    "description": "show interface implementation code lens",
    "version": "0.0.1",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "repository": "https://github.com/ctf0/implementation-lens",
    "main": "./out/main",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Implementation Lens",
            "properties": {
                "implementationLens.skipLanguages": {
                    "type": "array",
                    "default": [
                        "typescript",
                        "javascript"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "uniqueItems": true,
                    "description": "dont show code lens for this languages ids"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run esbuild-base -- --minify",
        "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
        "esbuild": "npm run esbuild-base -- --sourcemap",
        "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
        "watch": "npm run vscode:prepublish"
    },
    "devDependencies": {
        "@types/node": "^18.11.17",
        "@types/vscode": "^1.68.0",
        "esbuild": "^0.16.9",
        "typescript": "^4.9.4"
    }
}
